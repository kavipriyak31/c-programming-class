#include <stdio.h>
#include <string.h>

int main() {
    char str[50];
    int i, flag = 1;

    printf("Enter string: ");
    scanf("%s", str);

    for (i = 0; i < strlen(str) / 2; i++) {
        if (str[i] != str[strlen(str) - i - 1]) {
            flag = 0;
            break;
        }
    }

    if (flag)
        printf("Palindrome");
    else
        printf("Not Palindrome");

    return 0;
}
