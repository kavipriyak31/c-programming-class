#include <stdio.h>

int main() {
    char str[100], newstr[100];
    int i, j = 0;

    printf("Enter string: ");
    gets(str);

    for (i = 0; str[i]; i++) {
        if (str[i] != ' ')
            newstr[j++] = str[i];
    }
    newstr[j] = '\0';

    printf("Without spaces: %s", newstr);
    return 0;
}
