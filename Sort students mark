#include <stdio.h>
int main() {
    int a[5]={45,12,78,34,56}, i, j, temp;
    for(i=0;i<5;i++){
        for(j=i+1;j<5;j++){
            if(a[i]>a[j]){
                temp=a[i];
                a[i]=a[j];
                a[j]=temp;
            }
        }
    }
    printf("Sorted Marks:\n");
    for(i=0;i<5;i++)
        printf("%d ",a[i]);
    return 0;
}
